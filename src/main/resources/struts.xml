<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
  
<struts>
    <include file="struts-default.xml"/>
    <constant name = "struts.ui.theme" value="bootstrap"/>
    <package name = "default" extends="struts-default">
      <action name = "" class = "com.pe902_gaming.FantasyOutdoorTravelManager.GetTravelReport">
        <result name = "input">/WEB-INF/jsp/index.jsp</result>
        <result>/WEB-INF/jsp/index.jsp</result>
      </action>
      <action name = "GetTravelReport" class = "com.pe902_gaming.FantasyOutdoorTravelManager.GetTravelReport">
        <result name = "input">/WEB-INF/jsp/index.jsp</result>
        <result name = "success">/WEB-INF/jsp/travel_report.jsp</result>
        <result name = "error">/WEB-INF/jsp/index.jsp</result>
      </action>
      <action name = "CleanSlate" class = "com.pe902_gaming.FantasyOutdoorTravelManager.GetTravelReport">
        <result>/WEB-INF/jsp/index.jsp</result>
      </action>
    </package>
</struts>
